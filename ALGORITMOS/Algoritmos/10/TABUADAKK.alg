algoritmo "semnome"
// Função :
// Autor :
// Data : 04/11/2020
// Seção de Declarações 
var
cont, N, m: Inteiro
inicio
      cont <- 1
      Escreval ("Tabuada do: ")
      Leia (N)
           Repita
                 Escreval (N, " x", cont, " =", m)
                    cont <- cont + 1
                    m <- N*cont
           Ate (cont > 10)

           
      
      
fimalgoritmo
