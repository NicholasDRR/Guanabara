algoritmo "semnome"
// Função :
// Autor :
// Data : 04/11/2020
// Seção de Declarações 
var
n, s: Inteiro
resp: Caractere
inicio
s <- 0
  Repita
  Escreval ("Digite o número")
  Leia (n)
  s <- s + n
  Escreval (" Quer continuar [S/N]")
  Leia (resp)
  Ate ( resp = "N")
  Escreval (" A soma dos números é ", s)

  
  
fimalgoritmo
